}

/* Tabs container shouldn‚Äôt reserve height either (if present) */
#artists .wm-tabs .tabs-content,
#artists .wm-tabs [role="tabpanel"] {
  height: auto !important;
  min-height: 0 !important;
  overflow: visible !important;
}

/* Clear the spacer node some grid galleries create */
#artists .sqs-gallery.sqs-gallery-design-grid .gallery-grid .clear,
#artists .sqs-gallery.sqs-gallery-design-grid .gallery-grid > div[style*="clear"][style*="both"]{
  height: 0 !important; margin:0 !important; padding:0 !important; display:block !important;
}
